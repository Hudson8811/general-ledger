extends layouts/default

block vars
	-
		const page = {
		title: 'index.html'
		};




block main
	section.top-section.top-section--diag
		.ts-container.container
			.ts-flex
				.ts-flex__left
					h1.ts-heading Комплексная диагностика
						br.tbr
						|  корпоративных финансов

					.ts-text-desktop
						p Поможем найти причины проблем с финансами, и подготовим дорожную карту их решения
						p От 3-х дней и 50 тыс. руб
					.ts-text-mobile
						p Поможем найти причины проблем с финансами,
							br.tbr
							|  и подготовим дорожную карту их решения
						p От 3-х дней и 50 тыс. руб
					a.btn.btn--white.btn--with-right-arrow.ts-btn.ts-btn-desktop.js-modal-fancy-caller(data-fancy-src="#modal-consult") ПОЛУЧИТЬ КОНСУЛЬТАЦИЮ
						<svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg"><circle cx="10" cy="10" r="10" fill="white"/><path d="M8 5L13 10L8 15" stroke="#F26419" stroke-width="2"/></svg>

				.ts-flex__right
					img.ts-pic(src="/bitrix/templates/GL/img/diag.png" alt="")
					a.btn.btn--orange.btn--with-right-arrow.ts-btn.ts-btn-mobile.js-modal-fancy-caller(data-fancy-src="#modal-consult") ПОЛУЧИТЬ КОНСУЛЬТАЦИЮ
						<svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg"><circle cx="10" cy="10" r="10" fill="white"/><path d="M8 5L13 10L8 15" stroke="#F26419" stroke-width="2"/></svg>

	section.why-need-section
		.container
			h2.default-h2 Зачем нужна эта диагностика?
			.wns-block
				.wns-block__head
					.wns-block__head-title Отвечает на вопрос
					.wns-block__head-title Показывает возможность
				.wns-block__wrapper.js-wns-slider(data-counter-id='wns-counter')
					.wns-item
						.wns-item__digit 01
						.wns-item__question
							.wns-item__title-mobile Отвечает на вопрос
							span Где прибыль компании?
							.wns-item__arrows
								img(src="img/arrow-light-gray.svg" alt="")
								img(src="img/arrow-orange.svg" alt="")
						.wns-item__answer
							.wns-item__title-mobile Показывает возможность
							span Увеличить прибыль
					.wns-item
						.wns-item__digit 02
						.wns-item__question
							.wns-item__title-mobile Отвечает на вопрос
							span Деньги под контролем?
							.wns-item__arrows
								img(src="img/arrow-light-gray.svg" alt="")
								img(src="img/arrow-orange.svg" alt="")
						.wns-item__answer
							.wns-item__title-mobile Показывает возможность
							span Увеличить денежный поток
					.wns-item
						.wns-item__digit 03
						.wns-item__question
							.wns-item__title-mobile Отвечает на вопрос
							span Хватает ли оборотного капитала?
							.wns-item__arrows
								img(src="img/arrow-light-gray.svg" alt="")
								img(src="img/arrow-orange.svg" alt="")
						.wns-item__answer
							.wns-item__title-mobile Показывает возможность
							span Уменьшить дефицит <br> оборотных средств
					.wns-item
						.wns-item__digit 04
						.wns-item__question
							.wns-item__title-mobile Отвечает на вопрос
							span Учет корректный, цели достигаются?
							.wns-item__arrows
								img(src="img/arrow-light-gray.svg" alt="")
								img(src="img/arrow-orange.svg" alt="")
						.wns-item__answer
							.wns-item__title-mobile Показывает возможность
							span Достигнуть финансовой <br> цели месяца, квартала
					.wns-item
						.wns-item__digit 05
						.wns-item__question
							.wns-item__title-mobile Отвечает на вопрос
							span Компания соответствует условиям рынка?
							.wns-item__arrows
								img(src="img/arrow-light-gray.svg" alt="")
								img(src="img/arrow-orange.svg" alt="")
						.wns-item__answer
							.wns-item__title-mobile Показывает возможность
							span Обогнать конкурентов
					.wns-item
						.wns-item__digit 06
						.wns-item__question
							.wns-item__title-mobile Отвечает на вопрос
							span Используются все возможности для роста?
							.wns-item__arrows
								img(src="img/arrow-light-gray.svg" alt="")
								img(src="img/arrow-orange.svg" alt="")
						.wns-item__answer
							.wns-item__title-mobile Показывает возможность
							span Рост бизнеса, несмотря на кризисы
				.wns-mobile-counter
					#wns-counter.wns-mobile-counter__current 1
					.wns-mobile-counter__slash /
					.wns-mobile-counter__max.tablet 6
	section.need-condition-section
		.container
			h2.default-h2 Эта диагностика нужна Вам, если:
			.ncs-block.js-scroll-condition.js-ncs-slider(data-counter-id='ncs-counter')
				.ncs-block-item
					.ncs-block-item__circle(style='padding: 14px 15px 13px 16px;')
						img(src="img/icons/icon-7-white.svg" alt="")
					.ncs-block-item__title У бизнеса проблемы
					.ncs-block-item__explanation Прибыль есть, а денег нет. Затраты растут, доходность падает. Не хватает денег на платежи. Растет просроченная задолженность. Отчеты не
																			|  отражают реальной картины
					.ncs-block-item__note Диагностика покажет причины <br> проблем, чтобы устранить их
				.ncs-block-item
					.ncs-block-item__circle(style='padding: 18px 13px 18px 19px;')
						img(src="img/icons/icon-6-white.svg" alt="")
					.ncs-block-item__title Все хорошо, прибыль стабильная
					.ncs-block-item__explanation А это точно максимальная прибыль, которую может давать Ваш бизнес? Возможно, прибыль могла бы
																			|  быть на 10%, 15%, 20% больше за счет активации внутренних резервов?
					.ncs-block-item__note Диагностика покажет, в каком направлении искать внутренние <br> резервы, чтобы увеличить прибыль.
				.ncs-block-item
					.ncs-block-item__circle(style='padding: 16px 17px 18px 17px;')
						img(src="img/icons/icon-5-white.svg" alt="")
					.ncs-block-item__title Бизнес растет слишком медленно
					.ncs-block-item__explanation Рост бизнеса упирается в 3 ограничения: капитал, люди, мощность производства. Возможно, Ваш
																			|  бизнес уже достиг потолка в одном из этих направлений.
					.ncs-block-item__note Диагностика покажет, что ограничивает рост Вашего бизнеса и как убрать эти ограничения.
				.ncs-block-item
					.ncs-block-item__circle(style='padding: 9px 9px 15px 18px;')
						img(src="img/icons/icon-17-white.svg" alt="")
					.ncs-block-item__title Низкий возврат на инвестиции
					.ncs-block-item__explanation Вложили в бизнес десятки миллионов, а на выхлопе прибыль как от аренды однушки. Сотрудники
																			|  работают с утра до вечера, а денег больше не становится.
					.ncs-block-item__note Диагностика покажет, какие расходы можно сократить, чтобы увеличить рентабельность.
			.ncs-mobile-counter
				#ncs-counter.ncs-mobile-counter__current 1
				.ncs-mobile-counter__slash /
				.ncs-mobile-counter__max.tablet 4
	section.what-we-see-section
		.container
			h2.default-h2 Что смотрим на диагностике
			.wwss-block(data-counter-id='wwss-counter')
				.wwss-block-item
					.wwss-block-item__top
						.wwss-block-item__title Капитал
						.wwss-block-item__digit 01
					ul.wwss-block-item__list
						li Мин. цены капитала
						li Собственный капитал
						li Заемный капитал
						li Инвестиционные проекты
						li Оборотные активы
						li Внеоборотные активы
				.wwss-block-item
					.wwss-block-item__top
						.wwss-block-item__title Доходы и расходы
						.wwss-block-item__digit 02
					ul.wwss-block-item__list
						li Доходы
						li Ценовая политика
						li Выбор ассортимента
						li Калькуляции
						li Прямые и накладные затраты
						li Использование прибыли
				.wwss-block-item
					.wwss-block-item__top
						.wwss-block-item__title Движение <br> денежных средств
						.wwss-block-item__digit 03
					ul.wwss-block-item__list
						li Поступления
						li Платежи
						li Потребность в финансировании
						li Размещение свободных средств
						li ДДС по видам средств
						li Финансовая безопасность
				.wwss-block-item
					.wwss-block-item__top
						.wwss-block-item__title Организационные вопросы
						.wwss-block-item__digit 04
					ul.wwss-block-item__list
						li Внешняя среда
						li Внутренняя среда
						li Финансовые схемы
						li Финансовые инструменты
						li Договора, условия оплаты
						li Внутренняя организация ФЭС
				.wwss-block-item
					.wwss-block-item__top
						.wwss-block-item__title Экономические механизмы
						.wwss-block-item__digit 05
					ul.wwss-block-item__list
						li Бюджетирование
						li ЦФО, Бизнес-единицы
						li Внутренние цены
						li Нормирование
						li Мотивация
						li Анализ рисков
				.wwss-block-item
					.wwss-block-item__top
						.wwss-block-item__title Учет
						.wwss-block-item__digit 06
					ul.wwss-block-item__list
						li Достижение целей
						li Управленческий учет
						li Бухгалтерский учет
						li Налоговый учет
						li Консолидация отчетности
						li Автоматизация
			.wwss-mobile-counter
				#wwss-counter.wwss-mobile-counter__current 1
				.wwss-mobile-counter__slash /
				.wwss-mobile-counter__max.tablet 6
	section.what-result-section
		.container
			h2.default-h2 Что вы получите в результате
			.wrs-block
				.wrs-block-item
					.wrs-block-item__wrapper
						.wrs-block-item__content
							.wrs-block-item__digit 1
							.wrs-block-item__title Оцениваем <br> состояние бизнеса
							ul.wrs-block-item__list
								li Рассматриваем 6 сфер бизнеса - доходы и расходы, капитал, движение денег, учет, экономику и т. д.
								li Каждая сфера получает 2 оценки – насколько она важна для компании сейчас и насколько хорошо в ней идут дела.
								li Отняв от оценки важности оценку удовлетворенности, получаем потенциал роста сферы. Например, управленческий учет получил по важности 9, а по состоянию дел 4. Его потенциал роста 5.
						.wrs-block-item__picture.wrs-block-item__picture--big
							img(src="images/diag/1.png" alt="")
				.wrs-block-item.wrs-block-item
					.wrs-block-item__wrapper
						.wrs-block-item__content
							.wrs-block-item__digit 2
							.wrs-block-item__title.wrs-block-item__title--top Ранжируем проблемы <br> по важности
							ul.wrs-block-item__list
								li Улучшения в одной сфере, дают улучшения и в других, потому что в бизнесе все связано.
								li Чем больше потенциал роста, тем сильнее улучшения в этой сфере повлияют на улучшения во всей компании.
								li Поэтому мы выстраиваем все оцененные сферы по порядку убывания потенциала роста, чтобы увидеть ТОП-10.
						.wrs-block-item__picture
							img(src="images/diag/2.png" alt="")
				.wrs-block-item
					.wrs-block-item__wrapper
						.wrs-block-item__content
							.wrs-block-item__digit 3
							.wrs-block-item__title Оцениваем <br> состояние бизнеса
							ul.wrs-block-item__list
								li Видя потенциал роста каждой сферы в своем бизнесе, Вы решаете, что нужно улучшить в первую очередь
								li Часто бывает, что для улучшения сферы сначала нужно подготовить базу. Например, для улучшения автоматизации сначала надо внедрить единую методологию учета.
								li Мы делаем карту улучшений, зная, какие этапы должны быть сначала, чтобы улучшить эту сферу.
						.wrs-block-item__picture
	section.result-section.result-section--diag
		.rs-container.container
			h2.default-h2 Что Вы получите в результате
			.rs-block-mobile-digit-1 1
			.rs-block
				.rs-block-row.rs-block-row-1
					.rs-block-row__left.rs-block-row__left--text.customized-ul-wrap
						.rs-block-digit.rs-block-digit-1 1
						.rs-block-title Полную картину
							br.mbr
							|  финансов бизнеса
						ul.rs-block-ul
							li.rs-block-li Факторы риска – список проблемных точек в управлении финансами
								br.tbr
								|  и бизнесом, насколько они критичны
							li.rs-block-li Точки роста – где есть потенциал для роста, как его извлечь, какие проблемы при этом решить
							li.rs-block-li Ясность в финансах – что на что влияет, что менять, чтобы достичь финансовых целей
					.rs-block-row__right
						a.component-fancy.component-fancy-diag-rs-pic-1(href="/bitrix/templates/GL/images/diag/1-big.png" data-fancybox="optish-results-1")
							.component-fancy__inner
								img(src="/bitrix/templates/GL/images/diag/1.png", alt="")
				.rs-block-row.rs-block-row-2
					.rs-block-row__left
						a.component-fancy.component-fancy-diag-rs-pic-2(href="/bitrix/templates/GL/images/diag/2-big.png" data-fancybox="optish-results-1")
							.component-fancy__inner
								img(src="/bitrix/templates/GL/images/diag/2.png", alt="")
					.rs-block-row__right.rs-block-row__right--text.customized-ul-wrap
						.rs-block-digit.rs-block-digit-2 2
						.rs-block-title Дорожную карту решения проблем
						ul.rs-block-ul
							li.rs-block-li.rs-block-li__mobile-fsz16 Список сбоев и проблем в системе управления финансами и активами
							li.rs-block-li.rs-block-li__mobile-fsz16 Дорожную карту решения проблем - в каком направлении искать глубже, план конкретных действий

	section.we-will-suit-you-section(data-counter-id="ind-wwsy-1-counter").we-will-suit-you-section--diag__not-tab-slider
		.wwsy-container.container
			h2.default-h2 Когда нужна
				br.mbr
				|  такая диагностика
			.wwsy-block.wwsy-block--slider-tablet.js-wwsy-slider-tablet
				.wwsy-block-item
					.wwsy-block-item__pic(style="padding: 16px 22px;")
						img(src="/bitrix/templates/GL/img/icons/icon-1-white.svg" alt="")
					.wwsy-block-item__text
						.ah1 Компания разрослась
						p Вы видите, что текущие методы управления не справляются, нужна новая система управления процессами
				.wwsy-block-item
					.wwsy-block-item__pic(style="padding: 16px 29px;")
						img(src="/bitrix/templates/GL/img/icons/icon-12-white.svg" alt="")
					.wwsy-block-item__text
						.ah1 Компания собирается расти
						p Нужно улучшить управление финансами, чтобы снизить потери и избежать дорогих ошибок роста
				.wwsy-block-item
					.wwsy-block-item__pic(style="padding:16px 14px 16px 29px;")
						img(src="/bitrix/templates/GL/img/icons/icon-6-white.svg" alt="")
					.wwsy-block-item__text
						.ah1 Нужна объективная оценка
						p С финансовой системой все в порядке или есть серьезные проблемы? Если есть проблемы, как их устранить?
			.wwsy-block-mobile-counter.wwsy-block-tablet-counter
				#ind-wwsy-1-counter.wwsy-block-mobile-counter__current 1
				.wwsy-block-mobile-counter__slash /
				.wwsy-block-mobile-counter__max 3

	section.we-will-suit-you-section.we-will-suit-you-section--uprotch.we-will-suit-you-section--diag
		.wwsy-container.container
			h2.default-h2 Это нужно прямо
				br.mbr
				|   сейчас, если:
			.wwsy-uprotch.wwsy-uprotch--diag
				.wwsy-uprotch-item
					.wwsy-uprotch-item__number
						span 1
					.wwsy-uprotch-item__text Нужно увеличить прибыль
						br.dbr
						|  и сократить расходы
				.wwsy-uprotch-item
					.wwsy-uprotch-item__number
						span 2
					.wwsy-uprotch-item__text Нет твердой ясности, насколько хорошо идут дела у холдинга в целом и отдельных компаний
				.wwsy-uprotch-item
					.wwsy-uprotch-item__number
						span 3
					.wwsy-uprotch-item__text Нужно что-то менять в управлении финансами, но непонятно, что делать и нет времени, чтобы разобраться
				.wwsy-uprotch-item
					.wwsy-uprotch-item__number
						span 4
					.wwsy-uprotch-item__text Нет времени, чтобы найти и устранить причины проблем вместо борьбы с симптомами
				.wwsy-uprotch-item
					.wwsy-uprotch-item__number
						span 5
					.wwsy-uprotch-item__text Нужно увеличить стоимость бизнеса
				.wwsy-uprotch-item
					.wwsy-uprotch-item__number
						span 6
					.wwsy-uprotch-item__text Уменьшилась управляемость&nbsp;- недостаточно информации, затягивание действий, потери из-за несогласованности



	section.how-we-work-section.how-we-work-section--optish.how-we-work-section--diag
		.hww-container.container
			h2.default-h2 Как мы работаем
			.hww-block-mobile-tabs.hww-block-mobile-tabs--uprotch
				.hww-block-mobile-tab.hww-block-mobile-tab--active ШАГ 1
				.hww-block-mobile-tab ШАГ 2
				.hww-block-mobile-tab ШАГ 3
				.hww-block-mobile-tab ШАГ 4
				.hww-block-mobile-tab ШАГ 5
			.hww-block.hww-block--not-mansory.hww-block-non-mob-hidden.hww-block--optish.js-hww-block--not-mansory
				.hww-block-item.hww-block-item--active.hww-block-item--active-visible
					.hww-block-item-top
						.hww-block-item-top__pic
							img(src="/bitrix/templates/GL/images/step-1.jpg" alt="")
						.hww-block-item-top__text
							.hww-block-item-step Шаг 1
							.hww-block-item-duration 30-60 мин.
					.hww-block-item-bottom.customized-ul-wrap
						.hww-block-item-title Бесплатная консультация
						.hww-block-item-desc Уточняем задачу – какие цели стоят перед холдингом и&nbsp;отдельными компаниями, как производится управленческий и финансовый учет, как система корпоративных финансов связана с продажами, производством, логистикой и т.д.

				.hww-block-item
					.hww-block-item-top
						.hww-block-item-top__pic
							img(src="/bitrix/templates/GL/images/step-2.jpg" alt="")
						.hww-block-item-top__text
							.hww-block-item-step Шаг 2
							.hww-block-item-duration 1-8 часов
					.hww-block-item-bottom.customized-ul-wrap
						.hww-block-item-title Подписываем договор
						.hww-block-item-desc В договоре фиксируем:
						ul
							li Неразглашение
								br.mbr
								|  конфиденциальных данных
							li Условия работы, результат, сроки
